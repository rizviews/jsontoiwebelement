<?xml version="1.0"?>
<package >
  <metadata>
    <id>Ridia.TestAutomation.JsonToIWebElement</id>
    <version>1.1.1</version>
    <title>JsonToIWebElement</title>
    <authors>Rizwan</authors>
    <owners>Rizwan</owners>
	<projectUrl>https://github.com/rizviews/jsontoiwebelement</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Reads element definition from Json file and convert into Selenium IWebElement</description>
